<Window x:Class="SIMS_Projekat.DoctorView.DoctorHomePage"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SIMS_Projekat.DoctorView"
        mc:Ignorable="d"
        Title="DoctorHomePage" Height="750" Width="400" WindowState="Normal"
        Closing="DataWindow_Closing"
        WindowStartupLocation="CenterScreen" ResizeMode="NoResize">
    <Grid>
        <DockPanel x:Name="ActionBar" Grid.Row="0" Margin="0,0,0,630" Background="#FF5959"/>
        <DockPanel Margin="0,80,0,0" Background="#EEF2FF">
        </DockPanel>
        <Frame x:Name="MainFrame" NavigationUIVisibility="Hidden"/>
    </Grid>
</Window>
